local scripts = {
    "https://raw.githubusercontent.com/dracular111/01/main/Lock-Fps",
    "https://raw.githubusercontent.com/dracular111/01/main/Gift-Gem",
    "https://raw.githubusercontent.com/dracular111/01/main/Trackyummy",
    "https://raw.githubusercontent.com/dracular111/01/main/Script-Main-Reset",
}

for _, url in ipairs(scripts) do
    task.spawn(function()
        local ok, result = pcall(function()
            local code = game:HttpGet(url)
            local func = loadstring(code)
            if func then func() end
        end)
        if not ok then
            warn("⚠️ Lỗi khi load script từ: " .. url)
        else
            print("✅ Đã load: " .. url)
        end
    end)
    task.wait(2) -- nghỉ 0.2 giây tránh spam request
end
