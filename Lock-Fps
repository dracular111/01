getgenv()._CLEANUP = function()
    if _G.FpsLoop then
        task.cancel(_G.FpsLoop)
        print("[Lock-Fps] ðŸ§¹ Cleaned old loop")
    end
end

-- ðŸ§  Cáº¥u hÃ¬nh FPS báº¡n muá»‘n (vÃ­ dá»¥: 60 hoáº·c 5)
local TARGET_FPS = 5

_G.FpsLoop = task.spawn(function()
    while task.wait(1) do
        setfpscap(TARGET_FPS)
    end
end)

print("[Lock-Fps] âœ… FPS locked at", TARGET_FPS)
