local TeleportService = game:GetService("TeleportService")
local PlaceID = 126884695634066 -- Thay bằng ID của server/game mục tiêu nếu cần
local timeInterval = 1 * 60 -- 30 phút (30*60 giây); thay đổi giá trị này để điều chỉnh thời gian

while true do
    wait(timeInterval)
    TeleportService:Teleport(PlaceID)
end
