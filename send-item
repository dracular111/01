function senditem(name, iditem, type, amount, nameitem)
    local args =
    {
        [1] = name,
        [2] = "Chicken",
        [3] = type,
        [4] = iditem,
        [5] = amount or 1
    }
    if (game:GetService("ReplicatedStorage").Network["Mailbox: Send"]:InvokeServer(unpack(args))) then
        logdata(args[1], nameitem, args[5])
    end
end

