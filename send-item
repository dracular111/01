spawn(function()
    while wait() do
        if #save.Get().Inventory.Lootbox > 0 then
            for i, v in save.Get().Inventory.Lootbox do
                if v.id == "Locked Hype Egg" then
                    local args = {
                        [1] = i,
                        [2] = v._am or 1
                    }

                    game:GetService("ReplicatedStorage"):WaitForChild("Network"):WaitForChild("Lootbox: Open")
                        :InvokeServer(
                            unpack(
                                args))
                end
            end
        end
    end
end)
spawn(function()
    while wait() do
        if #save.Get().Inventory.Lootbox > 0 then
            for i, v in save.Get().Inventory.Lootbox do
                if v.id == "Locked Hype Egg" then
