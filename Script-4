task.wait(1)
-- Kiểm tra biến toàn cục để xác định script đã chạy hay chưa
if _G.SCRIPT4 then
    warn("Script đã được chạy trước đó!")
    return -- Dừng script nếu đã chạy
end

-- Đánh dấu script đã được chạy
_G.SCRIPT4 = true

-- Script của bạn
local TeleportService = game:GetService("TeleportService")
local PlaceID = 8737899170 -- Thay bằng ID của server/game mục tiêu nếu cần
local timeInterval = 120 * 60 -- 30 phút (30*60 giây); thay đổi giá trị này để điều chỉnh thời gian

while true do
    wait(timeInterval)
    TeleportService:Teleport(PlaceID)
end
