getgenv().Config = {
    ["Account Main"] = {"LiamV0rtexFire"},
    ["Pet"] = {"Lobster Thermidor"}
}
repeat
    wait()
until game:IsLoaded()
repeat
    wait()
until game.Players.LocalPlayer.Character
repeat
    wait()
    game:GetService("VirtualUser"):CaptureController()
    game:GetService("VirtualUser"):Button1Down(Vector2.new(0, 0))
until game.Players.LocalPlayer:GetAttribute("DataFullyLoaded") and
    game.Players.LocalPlayer:GetAttribute("Finished_Loading")
 function equipweapon(aq)
        local c6 = tostring(aq)
        local c7 = game.Players.LocalPlayer.Backpack:FindFirstChild(c6)
        local c8 = game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") or
                       game.Players.LocalPlayer.Character:WaitForChild("Humanoid")
        if c7 then
            c8:EquipTool(c7)
        end
    end
game.ReplicatedStorage.GameEvents.GiftPet.OnClientEvent:Connect(function(p_u_6, p7, p8)
    if table.find(getgenv().Config["Account Main"], game.Players.LocalPlayer.Name) then
        game.ReplicatedStorage.GameEvents.AcceptPetGift:FireServer(true, p_u_6)
    end
end)
function havepet()
    for i, v in game:GetService("Players").LocalPlayer.Backpack:GetChildren() do
        if v:IsA("Tool") and v:GetAttribute("ItemType") == "Pet" then
            for i1, v1 in getgenv().Config["Pet"] do
                if string.find(v.Name, v1) then
                    return v
                end
            end
        end
    end
    for i, v in game:GetService("Players").LocalPlayer.Character:GetChildren() do
        if v:IsA("Tool") and v:GetAttribute("ItemType") == "Pet" then
            for i1, v1 in getgenv().Config["Pet"] do
                if string.find(v.Name, v1) then
                    return v
                end
            end
        end
    end
end

function checkcntmain()
    local ReplicatedStorage = game:GetService("ReplicatedStorage")
    local DataService = require(ReplicatedStorage.Modules.DataService)
    local PetUtilities = require(ReplicatedStorage.Modules:WaitForChild("PetServices"):WaitForChild("PetUtilities"))
    local playerdata = DataService:GetData()
    local inventoryData = playerdata.PetsData and playerdata.PetsData.PetInventory and
                              playerdata.PetsData.PetInventory.Data or {}
    local cnt = 0
    for i, v in inventoryData do
        cnt = cnt + 1
    end
    return cnt
end
spawn(function()
    while wait() do
        if table.find(getgenv().Config["Account Main"], game.Players.LocalPlayer.Name) and checkcntmain() >= 60 then
            writefile(game.Players.LocalPlayer.Name .. ".txt", "Completed-Done Trading Main Account")

        end
    end
end)
spawn(function()
    while wait() do
        pcall(function()
            if not table.find(getgenv().Config["Account Main"], game.Players.LocalPlayer.Name) and not havepet() then
                writefile(game.Players.LocalPlayer.Name .. ".txt", "Completed-Trading Done")
                task.wait(3)
                game:Shutdown()
            end
            if havepet() and not table.find(getgenv().Config["Account Main"], game.Players.LocalPlayer.Name) then
                for i, v in getgenv().Config["Account Main"] do
                    if game.Players:FindFirstChild(v) then
                        equipweapon(havepet().Name)
                        if havepet():GetAttribute("d") then
                            havepet():SetAttribute("d", false)
                        end
                        local args = {"GivePet", game:GetService("Players"):WaitForChild(v)}
                        game:GetService("ReplicatedStorage"):WaitForChild("GameEvents")
                            :WaitForChild("PetGiftingService"):FireServer(unpack(args))
                        task.wait(3)
                    end
                end
            end

        end)
    end
end)
